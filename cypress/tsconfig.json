{
  "compilerOptions": {
    "noImplicitAny": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "ES5",
    "lib": [
      "es2015",
      "dom"
    ],
    "types": [
      "cypress",
      "node",
      "cypress-plugin-steps",
      "cy-verify-downloads"
    ]
  },
  "include": [
    "**/*.ts",
    "cypress/support/commands.ts",
    "cypress/support/commands.ts",
    "node_modules/cypress",
    "cypress/**/*.ts",
    "./cypress.d.ts",
    "../cypress.d.ts",
    "cypress.d.ts"
  ]
}